@model List<WebUI.Models.At.EventCount>

@Html.Partial("_breadcrumb")
@if (Model.Count() <= 0)
{
    @Html.Partial("_nodatafound")
}
else
{
    <div class="row">
        <div class="col">
            <table class="table table-sm table-responsive text-center text-secondary small">
                <thead class="table-light">
                    <tr>
                        <th>#</th>
                        <th class="text-start">Event Name</th>
                        <th>Month Year</th>
                        <th>Total</th>
                        <th>Present</th>
                        <th>Absent</th>
                        <th>Malpractice</th>
                        <th>Buffer</th>
                        <th>Pending</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @{ int i = 0;
                        foreach (var item in Model)
                        {
                            i++;
                                        <tr>
                                            <th scope="row">@i</th>
                                            <td class="text-start">@item.EventName</td>
                                            <td>@item.ExamMonthYear</td>
                                            <td>@item.Total</td>
                                            <td>@item.Present</td>
                                            <td>@item.Absent</td>
                                            <td>@item.Malpractice</td>
                                            <td>@item.Buffer</td>
                                            <td>@item.Pending</td>
                                            <td>@Html.ActionLink("View", "sessioncount", "dashboard", new { eid = item.Id }, new { @class = "btn btn-sm btn-info" })</td>
                                        </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
}


