@model WebUI.Models.Shared.Error

<div class="mt-5 d-flex flex-column align-items-center">
    <h1 class="display-1">@Model.Code</h1>
    <h1 class="display-6">@Model.Description</h1>
    <p class="text-muted">Sorry but the page you are looking for does not exist, have been removed. name changed or is temporarily unavailable.</p>
    <h6>
        @Html.ActionLink("Back to homepage", "index", "home", null, new { @class = "btn btn-sm btn-primary" })
    </h6>
</div>

